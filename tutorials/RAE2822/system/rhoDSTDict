FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      rhoDSTDict;
}
// *************************************************************************************** //


dependentVariable W; // W or deltaW

fluxScheme  Kurganov;

Jacobian    Roe;

timeIntegration
{
	timeIntegration	Single;
}

timeControls
{
    neighbouring	true;
    initialCo   	1;
    minCo		1;
    maxCo  	 	500;
}
